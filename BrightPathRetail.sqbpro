<?xml version="1.0" encoding="UTF-8"?><sqlb_project><db path="BrightPathRetail.db" readonly="0" foreign_keys="1" case_sensitive_like="0" temp_store="0" wal_autocheckpoint="1000" synchronous="2"/><attached/><window><main_tabs open="structure browser pragmas query" current="3"/></window><tab_structure><column_width id="0" width="300"/><column_width id="1" width="0"/><column_width id="2" width="100"/><column_width id="3" width="1627"/><column_width id="4" width="0"/><expanded_item id="0" parent="1"/><expanded_item id="1" parent="1"/><expanded_item id="2" parent="1"/><expanded_item id="3" parent="1"/></tab_structure><tab_browse><table title="products" custom_title="0" dock_id="1" table="4,8:mainproducts"/><dock_state state="000000ff00000000fd00000001000000020000000000000000fc0100000001fb000000160064006f0063006b00420072006f00770073006500310100000000ffffffff0000011300ffffff000000000000000000000004000000040000000800000008fc00000000"/><default_encoding codec=""/><browse_table_settings/></tab_browse><tab_sql><sql name="Q1*">--Q1: List all stores located in the &quot;West
SELECT StoreID, StoreName, OpeningDate 
FROM stores
WHERE Region = &quot;West&quot;;

</sql><sql name="Q2*">--Products in “Electronics” with price &gt; $500
SELECT ProductID, ProductName, UnitPrice
FROM products
WHERE Category =&quot;Electronics&quot;
AND UnitPrice &gt; 500;</sql><sql name="Q3*">--Total revenue by store
SELECT s.StoreID, s.StoreName,
SUM(sa.QuantitySold * p.UnitPrice) AS TotalRevenue
FROM Sales sa
JOIN Stores s ON sa.StoreID = s.StoreID
JOIN Products p ON sa.ProductID = p.ProductID
GROUP BY s.StoreID, s.StoreName
ORDER BY TotalRevenue DESC;

</sql><sql name="Q4*">--Top 5 products by revenue
SELECT p.ProductID, p.ProductName,
 SUM(sa.QuantitySold * p.UnitPrice) AS Revenue
 FROM Sales sa
 JOIN Products p ON sa.ProductID = p.ProductID
 GROUP BY p.ProductID, p.ProductName
 ORDER BY Revenue DESC
 LIMIT 5;</sql><sql name="SQL 5*">-- stores that opened before Jan1, 2020, total unit &gt;500
SELECT s.StoreID, s.StoreName, s.OpeningDate,
  SUM(sa.QuantitySold) AS TotalUnitsSold
  FROM Stores s
JOIN Sales sa ON s.StoreID = sa.StoreID
WHERE s.OpeningDate &lt; '2020-01-01'
GROUP BY s.StoreID, s.StoreName, s.OpeningDate
HAVING SUM(sa.QuantitySold) &gt; 500;</sql><sql name="Combined_Sales_Data*">SELECT sa.SaleID, sa.SaleDate, 
       s.StoreName, s.Region, s.OpeningDate,
       p.ProductName, p.Category, p.UnitPrice, 
       sa.QuantitySold,
       (sa.QuantitySold * p.UnitPrice) AS Revenue
FROM Sales sa
JOIN Stores s ON sa.StoreID = s.StoreID
JOIN Products p ON sa.ProductID = p.ProductID;</sql><current_tab id="5"/></tab_sql></sqlb_project>
